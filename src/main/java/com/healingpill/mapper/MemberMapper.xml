<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.healingpill.mapper.MemberMapper">
    <!-- 로그인 -->

    <select id = "memberLogin" resultType="com.healingpill.vo.MemberVO">
        select mem_id,mem_pwd from member where mem_id = #{mem_id} and mem_pwd = #{mem_pwd}
    </select>


</mapper>